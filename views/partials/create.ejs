<% if (folder) {%>
<form action="/myfiles/create/<%=folder.id%>" method="POST">
	<label for="folderName">Folder Name:</label
	><input type="text" name="folderName" id="folderName" />
	<button>Add Folder</button>
</form>
<%} else {%>
<form action="/myfiles/create" method="POST">
	<label for="folderName">Folder Name:</label
	><input type="text" name="folderName" id="folderName" />
	<button>Add Folder</button>
</form>
<%}%>
